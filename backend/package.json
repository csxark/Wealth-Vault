{
  "name": "wealth-vault-backend",
  "version": "1.0.0",
  "description": "Backend API for Wealth Vault financial management application",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --detectOpenHandles",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:migrate": "node -e 'import(\"./config/migrate.js\").then(m => m.migrate())'",
    "db:fix-schema": "node db/migrations/fix-schema-consistency.js",
    "db:validate": "node -e 'import(\"./utils/schemaValidation.js\").then(v => console.log(\"Schema validation loaded\"))",
    "tokens:cleanup": "node -e 'import(\"./jobs/tokenCleanup.js\").then(j => j.runCleanup())'"
  },
  "dependencies": {
    "@google/genai": "^1.35.0",
    "axios": "^1.7.2",
    "plaid": "^21.0.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "drizzle-orm": "^0.45.1",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^8.2.1",
    "express-validator": "^7.0.1",
    "file-type": "^19.0.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "openai": "^6.15.0",
    "pdfkit": "^0.15.0",
    "postgres": "^3.4.7",
    "qrcode": "^1.5.4",
    "rate-limit-redis": "^4.2.0",
    "react-hot-toast": "^2.6.0",
    "redis": "^4.7.1",
    "speakeasy": "^2.0.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^10.0.0",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^5.0.0",
    "xss": "^1.0.15",
    "zxcvbn": "^4.4.2"
  },
  "devDependencies": {
    "drizzle-kit": "^0.31.8",
    "jest": "^29.7.0",
    "nodemon": "^3.1.11",
    "supertest": "^6.3.3"
  },
  "keywords": [
    "mern",
    "express",
    "postgresql",
    "drizzle",
    "supabase",
    "financial",
    "budgeting"
  ],
  "author": "Wealth Vault Team",
  "license": "MIT"
}
